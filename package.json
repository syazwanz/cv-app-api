{
  "name": "cv-app-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e .env.development -- nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "dotenv -e .env.test -- npx prisma migrate deploy && prisma db seed && jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "cors": "^2.8.5",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prisma": "^5.15.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "dotenv -e .env.test -- ts-node prisma/seed.ts"
  }
}
